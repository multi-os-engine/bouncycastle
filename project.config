[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[project]
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group approvers-platform-external-bouncycastle
	label-Code-Review = -2..+1 group Android Maintainers
	label-Verified = -1..+0 group Android Maintainers
[access "refs/heads/master"]
	label-Verified = -1..+1 group Android Maintainers
	submit = group Android Maintainers
[access]
	inheritFrom = Platform-Projects
[access "refs/heads/upstream-master"]
	pushTag = group mdb/android-runtime
